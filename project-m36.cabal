                   ,cassava ==0.4.*
                   ,text
                   ,bytestring
                   ,deepseq
                   ,deepseq-generics
                   ,vector
                   ,parallel
                   ,monad-parallel
                   ,transformers
                   ,gnuplot
                   ,binary
                   ,filepath
                   ,directory
                   ,vector-binary-instances
                   ,temporary
                   ,stm
                   ,time
                   ,hashable-time
                   ,old-locale
                   ,attoparsec
                   ,either
                   ,base64-bytestring
                   ,data-interval
                   ,extended-reals
                   ,network-transport
--required for persistent db driver
                   ,persistent >= 2.2 && < 2.3
                   ,persistent-template
                   ,aeson
                   ,path-pieces
                   ,conduit
                   ,resourcet
                   ,http-api-data
--needed for remote access                   
                   ,distributed-process-client-server
                   ,distributed-process-extras
